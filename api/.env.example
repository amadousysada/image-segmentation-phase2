# Configuration MLflow
MLFLOW_TRACKING_URI=http://localhost:5000
RUN_ID=your_run_id_here

# Configuration de debugging pour le modèle
TF_CPP_MIN_LOG_LEVEL=2
KERAS_VERBOSE=1

# Options de compatibilité
FORCE_MODEL_RELOAD=false
SKIP_MODEL_VALIDATION=false

# Timeout pour les requêtes MLflow (en secondes)
MLFLOW_HTTP_REQUEST_TIMEOUT=6000